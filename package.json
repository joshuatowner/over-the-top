{
  "name": "over-the-top",
  "version": "0.8.0",
  "description": "A minimal Electron application written with Typescript",
  "main": "dist/bundle.js",
  "scripts": {
    "build:worker": "tsc src/old/worker/worker.ts --outDir dist",
    "build:prod": "webpack --config webpack.prod.config.js",
    "build:dev": "webpack --config webpack.dev.config.js",
    "build:dev:watch": "webpack --config webpack.dev.config.js --watch",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "yarn run build:dev && electron dist/bundle.js",
    "start:watch": "yarn run build:dev:watch & electron dist/bundle.js",
    "start:prod": "yarn run build:prod && electron ./dist/bundle.js",
    "package-curent": "electron-packager . --out=out",
    "package-linux": "electron-packager . --out=out --platform=linux --arch=x64",
    "package-win": "electron-packager . --out=out --platform=win32 --arch=x64",
    "package-mac": "electron-packager . --out=out --platform=darwin --arch=x64",
    "clear-packages": "rm -rf out/* dist/*",
    "package": "yarn clear-packages && yarn build:prod && yarn package-linux && yarn package-win && yarn package-mac"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/color": "^3.0.3",
    "@types/deep-equal": "^1.0.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/react-icons": "^3.0.0",
    "@types/styled-components": "^5.1.26",
    "@types/two.js": "0.7.1",
    "@types/uuid": "^9.0.1",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "electron": "^24.1.3",
    "electron-packager": "^17.1.1",
    "electron-reloader": "^1.2.3",
    "eslint": "^8.39.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "mini-css-extract-plugin": "^2.7.5",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "ttf-loader": "^1.0.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "color": "^4.2.3",
    "deep-equal": "^2.2.1",
    "parse-domain": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-perfect-scrollbar": "^1.5.8",
    "styled-components": "^5.3.10",
    "systeminformation": "^5",
    "two.js": "0.7.0-stable.1",
    "uuid": "^9.0.0"
  }
}
