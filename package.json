{
  "name": "over-the-top",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "main": "electron.js",
  "scripts": {
    "build:worker": "tsc src/old/worker/worker.ts --outDir dist",
    "build:prod": "webpack --config webpack.prod.config.js",
    "build:dev": "webpack --config webpack.dev.config.js",
    "build:dev:watch": "webpack --config webpack.dev.config.js --watch",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "yarn run build:dev && electron electron.js",
    "start:watch": "yarn run build:dev:watch & electron electron.js",
    "start:prod": "yarn run build:prod && electron ./dist/bundle.js",
    "package-curent": "electron-packager . --out=out",
    "package-linux": "electron-packager . --out=out --platform=linux --arch=x64",
    "package-win": "electron-packager . --out=out --platform=win32 --arch=x64",
    "package-mac": "electron-packager . --out=out --platform=darwin --arch=x64",
    "clear-packages": "rm -rf out/*",
    "package": "yarn clear-packages && yarn build:prod && yarn package-linux && yarn package-win && yarn package-mac"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.2",
    "@types/two.js": "^0.7.2",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "electron": "^9.0.4",
    "electron-packager": "^15.0.0",
    "electron-reloader": "^1.0.1",
    "eslint": "^6.1.2",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.3.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.10",
    "ttf-loader": "^1.0.2",
    "typescript": "^3.9.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "worker-loader": "^3.0.5"
  },
  "dependencies": {
    "color": "^3.1.2",
    "parse-domain": "^3.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-perfect-scrollbar": "^1.5.8",
    "styled-components": "^5.1.1",
    "systeminformation": "^4.29.3",
    "two.js": "^0.7.0-stable.1"
  }
}
