{
  "name": "over-the-top",
  "version": "0.8.1",
  "description": "A minimal Electron application written with Typescript",
  "main": "dist/main.js",
  "scripts": {
    "build:worker": "tsc src/old/worker/worker.ts --outDir dist",
    "build:prod": "webpack --config webpack.prod.config.js",
    "build:dev": "webpack --config webpack.dev.config.js",
    "build:dev:watch": "webpack --config webpack.dev.config.js --watch",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "yarn run build:dev && electron dist/main.js",
    "start:watch": "yarn run build:dev:watch & electron dist/main.js",
    "start:prod": "yarn run build:prod && electron ./dist/main.js",
    "package-curent": "electron-packager . --out=out",
    "package-linux": "electron-packager . --out=out --platform=linux --arch=x64",
    "package-win": "electron-packager . --out=out --platform=win32 --arch=x64",
    "package-mac": "electron-packager . --out=out --platform=darwin --arch=x64",
    "clear-packages": "rm -rf out/* dist/*",
    "package": "yarn clear-packages && yarn build:prod && yarn package-linux && yarn package-win && yarn package-mac"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/color": "^4.2.0",
    "@types/deep-equal": "^1.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-icons": "^3.0.0",
    "@types/styled-components": "^5.1.26",
    "@types/two.js": "0.7.9",
    "@types/uuid": "^10.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "electron": "^37.2.0",
    "electron-packager": "^17.1.1",
    "electron-reloader": "^1.2.3",
    "eslint": "^9.30.1",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^5.1.0",
    "mini-css-extract-plugin": "^2.7.5",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@types/fkill": "^6.0.1",
    "color": "^5.0.0",
    "deep-equal": "^2.2.1",
    "fkill": "^9.0.0",
    "overlayscrollbars": "^2.11.4",
    "overlayscrollbars-react": "^0.5.6",
    "parse-domain": "^8.2.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-perfect-scrollbar": "^1.5.8",
    "styled-components": "^6.1.19",
    "systeminformation": "^5.27.7",
    "two.js": "0.8.19",
    "uuid": "^11.1.0",
    "voronoijs": "^1.0.0"
  }
}
